{
  "name": "cs-rx",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && preact test",
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintIgnore": ["build"],
  "eslintConfig": {
    "extends": ["eslint-config-synacor", "prettier", "prettier/react"],
    "rules": {
      "jsx-a11y/href-no-hash": 0,
      "import/prefer-default-export": 0,
      "react/no-unknown-property": [
        2,
        {
          "ignore": ["class"]
        }
      ],
      "react/sort-comp": [
        2,
        {
          "order": [
            "properties",
            "constructor",
            "static-methods",
            "everything-else",
            "/^(on|handle).+$/",
            "lifecycle",
            "render"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.5.3",
    "preact-cli": "^1.4.1",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "date-fns": "^1.28.5",
    "decko": "^1.2.0",
    "preact": "^8.2.1",
    "rxjs": "^5.4.3"
  }
}
